<h1>Editing Patient Evaluation</h1>

<% form_for([@patient, @patient_evaluation]) do |f| %>
  <%= f.error_messages %>

  <%= render :partial => 'patient_evaluation', :locals => {:f => f} %>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= show_link patient_patient_evaluation_path(@patient, @patient_evaluation) %>