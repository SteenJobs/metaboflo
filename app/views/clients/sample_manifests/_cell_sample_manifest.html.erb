<div class="cell_sample_manifest fields box">
<table class="list">
	<tr>
		<td><%= f.label :tube_id %></td><td><%= f.text_field :tube_id, :size => 3, :disabled => true%></td>
		<td><%= f.label :barcode %></td><td><%= f.text_field :barcode, :size => 20, :disabled => true %></td>
		<td><%= f.label :cell_line %></td><td><%= f.text_field :cell_line, :size => 15 %></td>
	</tr>
	<tr>
		<td><%= f.label :viable_cells %></td><td><%= f.text_field :viable_cells, :size => 15 %></td>
		<td><%= f.label :group_id %></td><td><%= f.text_field :group_id, :size => 2 %></td>
	</tr>
	<tr>
		<td><%= f.label :test_codes %></td>
		<td colspan=5>
			<%= f.label :module_1%>&ensp;<%= f.check_box :module_1 %>
			<%= f.label :module_2%>&ensp;<%= f.check_box :module_2 %>
			<%= f.label :module_3%>&ensp;<%= f.check_box :module_3 %>
			<%= f.label :module_4%>&ensp;<%= f.check_box :module_4 %>
			<%= f.label :module_5%>&ensp;<%= f.check_box :module_5 %>
			<%= f.label :gc_fap%>&ensp;<%= f.check_box :gc_fap %>
			<%= f.label :ss_1%>&ensp;<%= f.check_box :ss_1 %>
			<%= f.label :ss_2%>&ensp;<%= f.check_box :ss_2 %>
		</td>
	</tr>
	<tr>
		<td colspan=2><%= remove_child_link("Remove", f) %></td>
	</tr>
</table>
</div>