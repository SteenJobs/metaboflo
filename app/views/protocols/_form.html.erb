<%= form.error_messages %>

<fieldset style="width: 50%">
  <legend>Protocol Details</legend>
  <p><%= form.text_field :name, :required => true %></p>
  <p><%= form.text_field :version, :size => 5 %></p>
  <p><%= form.text_area :comments %></p>
  <p><%= form.file_field :uploaded_data, :label => 'File', :required => true %></p>
</fieldset>