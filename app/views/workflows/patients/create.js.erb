close_patient_dialog();
set_patient_code("<%= @test_subject.code %>");

<% @sample = @test_subject.samples.first %>
reset_sample_options('<%= options_for_select([['', ''], [@sample.to_s, @sample.id]], @sample.id).gsub(/\n/, '') %>');

set_sample_readonly("<%= escape_javascript(render :partial => '/workflows/samples/show') %>", <%= @sample.id %>);

<% if flash[:notice] %>set_flash('notice', "<%= flash[:notice] %>");<% end %>

<% @test_subject = TestSubject.new %>
new_patient_form("<%= escape_javascript(render :partial => 'form') %>");
