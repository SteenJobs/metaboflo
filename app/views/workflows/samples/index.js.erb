reset_sample_options('<%= options_for_select([['', '']] + @samples.collect { |s| [s.to_s, s.id] }).gsub(/\n/, '') %>');

<% if @samples.empty? %>
	new_sample_form();
<% else %>
	new_sample_form($.ajax("<%= new_workflows_patient_sample_path @samples.first.test_subject, :format => :js %>"));
<% end %>
