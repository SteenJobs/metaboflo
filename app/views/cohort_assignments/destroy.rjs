if @successful
  page.replace_html "assignable-associations", :partial => 'cohort_assignment', :collection => @cohort.cohort_assignments
  page.replace_html 'flash', "<p class=\"notice\">Removed #{humanized_type(@cohort.assignable_type).downcase} from cohort successfully</p>"
else
  page.replace_html 'flash', "<p class=\"error\">Error removing #{humanized_type(@cohort.assignable_type).downcase} to cohort</p>"
end
