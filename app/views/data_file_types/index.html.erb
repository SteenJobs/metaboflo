<h1>Data File Types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
  </tr>

<% for data_file_type in @data_file_types %>
  <tr>
    <td><%=h data_file_type.name %></td>
    <td><%=h data_file_type.description %></td>
    <td><%= link_to 'Show', data_file_type %></td>
    <td><%= link_to 'Edit', edit_data_file_type_path(data_file_type) %></td>
    <td><%= link_to 'Destroy', data_file_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New data_file_type', new_data_file_type_path %>
