<h1>Editing meal for <%= TestSubject.title %> <%= @test_subject.code %></h1>

<%= labelled_tabular_form_for([@test_subject, @meal]) do |f| %>
  <%= f.error_messages %>

  <%= render :partial => f %>

  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= show_link test_subject_meal_path(@test_subject, @meal) %>