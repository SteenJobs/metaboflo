<h1>Editing Lab Test</h1>

<%= knoxy_form_for([@test_subject, @lab_test]) do |form| %>

  <%= render :partial => 'form', :locals => { :form => form } %>

  <%= form.error_messages %>

  <p>
    <%= form.submit "Update" %>
  </p>
<% end %>

<%= show_link test_subject_lab_test_path(@test_subject, @lab_test) %>